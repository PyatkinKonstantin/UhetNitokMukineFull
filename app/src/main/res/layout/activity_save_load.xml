<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activityes.SaveLoadActivity">



    <ImageView
        android:id="@+id/header_SaveLoad"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:foreground="@drawable/headerempty"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/iv_save_back"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="20dp"
        android:foreground="#E6E6E6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_SaveLoad" />

    <!--<ImageButton
        android:id="@+id/bt_logoff"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="356dp"
        android:background="@drawable/logout"
        android:onClick="onClickLogoff"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />-->

    <!--<ImageButton
        android:id="@+id/bt_save"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="70dp"
        android:onClick="saveToDevic"
        android:background="@drawable/mobile_phone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_SaveLoad" />-->

    <Button
        android:id="@+id/bt_save"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:backgroundTint="#00000000"
        android:onClick="saveToDevic"
        android:padding="9dp"
        app:icon="@drawable/save__2_"
        app:iconSize="30dp"
        app:iconTint="#000000"
        app:layout_constraintStart_toStartOf="@+id/iv_save_back"
        app:layout_constraintTop_toTopOf="@+id/iv_save_back" />

    <ImageView
        android:id="@+id/iv_load_back"
        android:layout_width="0dp"
        android:layout_height="150dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="36dp"
        android:foreground="#E6E6E6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/readFromFireBase"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="15dp"
        android:backgroundTint="#00000000"
        android:onClick="loadFromFBase"
        android:padding="8dp"
        app:icon="@drawable/lfc"
        app:iconSize="35dp"
        app:iconTint="#000000"
        app:layout_constraintEnd_toEndOf="@+id/iv_load_back"
        app:layout_constraintTop_toTopOf="@+id/iv_load_back" />

    <Button
        android:id="@+id/bt_load"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="15dp"
        android:backgroundTint="#00000000"
        android:onClick="loadFromDevic"
        android:padding="8dp"
        app:icon="@drawable/mobile_phone"
        app:iconSize="35dp"
        app:iconTint="#000000"
        app:layout_constraintEnd_toEndOf="@+id/iv_load_back"
        app:layout_constraintTop_toBottomOf="@+id/readFromFireBase" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:text="@string/save_to_device"
        android:fontFamily="@font/akadem"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/bt_save"
        app:layout_constraintEnd_toEndOf="@+id/iv_save_back"
        app:layout_constraintStart_toEndOf="@+id/bt_save"
        app:layout_constraintTop_toTopOf="@+id/bt_save" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="20dp"
        android:gravity="right"
        android:text="@string/loadText"
        android:fontFamily="@font/akadem"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/bt_load"
        app:layout_constraintEnd_toStartOf="@+id/bt_load"
        app:layout_constraintStart_toStartOf="@+id/iv_load_back"
        app:layout_constraintTop_toTopOf="@+id/bt_load" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:gravity="right"
        android:text="@string/load_from_cloud"
        android:fontFamily="@font/akadem"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/readFromFireBase"
        app:layout_constraintEnd_toStartOf="@+id/readFromFireBase"
        app:layout_constraintStart_toStartOf="@+id/iv_load_back"
        app:layout_constraintTop_toTopOf="@+id/readFromFireBase" />

    <Button
        android:id="@+id/bt_home_save_load"
        android:layout_width="50dp"
        android:layout_height="60dp"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="5dp"
        android:padding="13dp"
        android:backgroundTint="#00000000"
        app:icon="@drawable/ba"
        app:iconSize="26dp"
        app:iconTint="#000000"
        android:onClick="onClickHomeFromSaveLoad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_user_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/akadem"
        android:hint=""
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/googleAccountLPicture"
        app:layout_constraintTop_toTopOf="parent" />

    <!--app:icon="@drawable/logout"-->

    <Button
        android:id="@+id/googleAccountLPictureButton"
        android:layout_width="51dp"
        android:layout_height="60dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="#00000000"
        android:onClick="onClickHomeFromSaveLoad"
        android:padding="13dp"
        app:iconSize="26dp"
        app:iconTint="#000000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/googleAccountLPicturePod"
        android:layout_width="36dp"
        android:layout_height="34dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="13dp"
        android:background="#A3A3A3"
        app:layout_constraintStart_toStartOf="@+id/googleAccountLPictureButton"
        app:layout_constraintTop_toTopOf="@+id/googleAccountLPictureButton"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/googleAccountLPicture"
        android:layout_width="30dp"
        android:layout_height="28dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="16dp"
        android:onClick="onClickLogoff"
        app:layout_constraintStart_toStartOf="@+id/googleAccountLPictureButton"
        app:layout_constraintTop_toTopOf="@+id/googleAccountLPictureButton"
        tools:srcCompat="@tools:sample/avatars" />

    <!--<ImageView
        android:id="@+id/googleAccountLPictureDefault"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="436dp"
        android:background="@drawable/logout"
        android:onClick="onClickLogoff"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />-->




</androidx.constraintlayout.widget.ConstraintLayout>